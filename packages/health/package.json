{
  "name": "ts-health",
  "type": "module",
  "version": "0.0.0",
  "description": "TypeScript library for health, fitness & smartwatch data. Oura Ring, WHOOP, Apple Health, Fitbit, Garmin, Polar, Suunto, Coros, Wahoo â€” with FIT parsing, training metrics, and more.",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/ts-health#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/ts-health.git"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/ts-health/issues"
  },
  "keywords": [
    "oura",
    "oura-ring",
    "whoop",
    "fitbit",
    "apple-health",
    "garmin",
    "polar",
    "suunto",
    "coros",
    "wahoo",
    "smartwatch",
    "health",
    "fitness",
    "sleep",
    "hrv",
    "readiness",
    "recovery",
    "training",
    "heart-rate",
    "fit-file",
    "gpx",
    "tcx",
    "training-load",
    "tss",
    "vo2max",
    "typescript",
    "bun"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./drivers": {
      "types": "./dist/src/drivers/index.d.ts",
      "import": "./dist/src/drivers/index.js"
    },
    "./analysis": {
      "types": "./dist/src/analysis/index.d.ts",
      "import": "./dist/src/analysis/index.js"
    },
    "./*": {
      "import": "./dist/*"
    }
  },
  "module": "./dist/src/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "health": "./dist/bin/cli.js"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "bun --bun build.ts && bun run compile",
    "compile": "bun build ./bin/cli.ts --compile --minify --outfile bin/health",
    "compile:all": "bun run compile:linux-x64 && bun run compile:linux-arm64 && bun run compile:windows-x64 && bun run compile:darwin-x64 && bun run compile:darwin-arm64",
    "compile:linux-x64": "bun build ./bin/cli.ts --compile --minify --target=bun-linux-x64 --outfile bin/health-linux-x64",
    "compile:linux-arm64": "bun build ./bin/cli.ts --compile --minify --target=bun-linux-arm64 --outfile bin/health-linux-arm64",
    "compile:windows-x64": "bun build ./bin/cli.ts --compile --minify --target=bun-windows-x64 --outfile bin/health-windows-x64.exe",
    "compile:darwin-x64": "bun build ./bin/cli.ts --compile --minify --target=bun-darwin-x64 --outfile bin/health-darwin-x64",
    "compile:darwin-arm64": "bun build ./bin/cli.ts --compile --minify --target=bun-darwin-arm64 --outfile bin/health-darwin-arm64",
    "zip": "bun run zip:all",
    "zip:all": "bun run zip:linux-x64 && bun run zip:linux-arm64 && bun run zip:windows-x64 && bun run zip:darwin-x64 && bun run zip:darwin-arm64",
    "zip:linux-x64": "zip -j bin/health-linux-x64.zip bin/health-linux-x64",
    "zip:linux-arm64": "zip -j bin/health-linux-arm64.zip bin/health-linux-arm64",
    "zip:windows-x64": "zip -j bin/health-windows-x64.zip bin/health-windows-x64.exe",
    "zip:darwin-x64": "zip -j bin/health-darwin-x64.zip bin/health-darwin-x64",
    "zip:darwin-arm64": "zip -j bin/health-darwin-arm64.zip bin/health-darwin-arm64",
    "lint": "bunx --bun pickier .",
    "lint:fix": "bunx --bun pickier . --fix",
    "fresh": "bunx rimraf node_modules/ bun.lock && bun i",
    "changelog": "bunx changelogen --output CHANGELOG.md",
    "prepublishOnly": "bun --bun run build && bun run compile:all && bun run zip",
    "release": "bun run changelog && bunx bumpp package.json --all",
    "test": "bun test"
  },
  "dependencies": {
    "ts-watches": "https://pkg.pr.new/stacksjs/ts-watches@3fdd0f3"
  },
  "devDependencies": {
    "@stacksjs/clapp": "^0.2.0",
    "bun-plugin-dtsx": "^0.9.12",
    "bunfig": "^0.15.6"
  },
  "lint-staged": {
    "*.{js,ts}": "bunx pickier . --fix"
  }
}
